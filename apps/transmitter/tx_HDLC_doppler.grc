import numpy as np
import pylab
from gnuradio import gr
import pmt
import time
import serial


class msg_block(gr.basic_block):  # other base classes are basic_block, decim_block, interp_block
	"""This block sends commands within a CSP frame when a message is received on the input.
	
	Inputs:
	- ptt: when 0, tx is off; when 1, tx is on.
	- cmd_type: selects the command type from the 5 available types.
	- choose_ram_cmd: commands that change RAM parameters
	- choose_reset_cmd: commands to reset modules of the satellite
	- choose_ax100_cmd: commands to control the AX100 module
	- choose_htr_cmd: commands to control the battery heater
	- choose_pld_cmd: commands to take and download images with the payload module
	- command_data: contains the data that will be sent in a command, for example the RTC value
	- hour, minute, day, month, year: part of the RTC

	Outputs:
	- PMT type message containing a uint8 vector that contains the command encapsulated in the CSP frame.
	"""

	def __init__(self, ptt = 0, test_cmd = 0, cmd_type = 0, choose_ram_cmd = 0, choose_reset_cmd = 0, \
	choose_ax100_cmd = 0, choose_htr_cmd = 0, choose_pld_cmd = 0, packet_len = 0,   \
	command_data = 0, hour = 0, minute = 0, day = 0, month = 0, year = 0):
		"""arguments to this function show up as parameters in GRC"""
		gr.basic_block.__init__(
			self,
			name='Generate Command',   # will show up in GRC
			in_sig=None,
			out_sig=None
		)

		# serial port is used to control the USB relay that controles the switch from RX to TX
		# if /dev/ttyACM1 throws any errors, check to see if dialout is within your group permissions. Use:
		# $ groups ${USER}
		# $ sudo gpasswd --add ${USER} dialout

		# UNCOMMENT THESE LINES WHEN USING THE RF RELAY IN THE GROUND CONTROL STATION
		# SINCE THE RF RELAY IS CONTROLLED BY THE USB RELAY
		self.ser = serial.Serial(
			port='/dev/ttyUSB1',
			baudrate=9600,
			parity=serial.PARITY_NONE,
			stopbits=serial.STOPBITS_ONE,
			bytesize=serial.EIGHTBITS
		)

		# if an attribute with the same name as a parameter is found,
		# a callback is registered (properties work, too).		
		self.ptt = ptt
		self.cmd_type = cmd_type
		self.test_cmd = test_cmd
		self.choose_ram_cmd = choose_ram_cmd
		self.choose_reset_cmd = choose_reset_cmd
		self.choose_ax100_cmd = choose_ax100_cmd
		self.choose_htr_cmd = choose_htr_cmd
		self.choose_pld_cmd = choose_pld_cmd
		self.packet_len = packet_len
		self.command_data = command_data
		self.hour = hour
		self.minute = minute
		self.day = day
		self.month = month
		self.year = year

		self.message_port_register_out(pmt.intern('msg_out'))
		self.message_port_register_in(pmt.intern('msg_in'))
		self.set_msg_handler(pmt.intern('msg_in'), self.handle_msg)

	def handle_msg(self, msg):
		if self.ptt == 1: # only transmits when PTT button is pressed 
			
			# SET RELAYS FOR TX MODE
			self.ser.write(bytearray([0xa0,0x01,0x01,0xa2]))	# sets relays in TX mode
			time.sleep(0.1)									# give relays time to switch

			# RESET PAYLOD CMD VERIFICATION FLAG
			pldCmdFlag = False
			# RESET RESET CMD VERIFICATION FLAG
			rstCmdFlag = False

			# GENERATE CSP HEADER	
				# priority = 1
				# source = 7
				# destination = 1
				# destination port = 25		
				# source port = 7
				# reserved = 0
				# HMAC = 0
				# XTEA = 0
				# RDP = 0
				# CRC = 0

			# CSP: 01 00111 00001 011001 000111 00000000

			priority = int('{0:02b}'.format(1), 2)	
			source = int('{0:05b}'.format(7), 2)	
			destination = int('{0:05b}'.format(1), 2)
			destination_port = int('{0:06b}'.format(25), 2)
			source_port = int('{0:06b}'.format(7), 2)
			reserved = int('{0:04b}'.format(0), 2)
			hmac = int('{0:01b}'.format(0), 2)
			xtea = int('{0:01b}'.format(0), 2)
			RDP = int('{0:01b}'.format(0), 2)
			CRC = int('{0:01b}'.format(0), 2)

			CSP_header = (priority &lt;&lt; 30) | (source &lt;&lt; 25) | (destination &lt;&lt; 20) | (destination_port &lt;&lt; 14) | \
			(source_port &lt;&lt; 8) | (reserved &lt;&lt; 4) | (hmac &lt;&lt; 3) | (xtea &lt;&lt; 2) | (RDP &lt;&lt; 1) | CRC

			byte1 = np.array([CSP_header &amp; 0xFF],np.uint8)
			byte2 = np.array([(CSP_header &gt;&gt; 8) &amp; 0xFF],np.uint8) 
			byte3 = np.array([(CSP_header &gt;&gt; 16) &amp; 0xFF],np.uint8)
			byte4 = np.array([(CSP_header &gt;&gt; 24) &amp; 0xFF],np.uint8)

			# GENERATE VERIFICATION CODE
			# verification code is 0x6F, 0x5D

			verByte1 = np.array([0x6f],np.uint8)
			verByte2 = np.array([0x5d],np.uint8)


			# COMMAND DICTIONARIES
			# TAB-CDH-001 contains all the commands listed below

			# commands to change ram parameters
			# 0 = CDHS Cycle Time
			# 1 = CDHS WDT Time
			# 2 = ADM SOC Limit
			# 3 = ADCS SOC Limit
			# 4 = COMMS SOC Limit
			# 5 = PLD SOC Limit
			# 6 = HEATER Cycle Time
			# 7 = HEATER Emergency ON Time
			# 8 = HEATER Emergency OFF Time
			# 9 = ADC Cycle Time
			# 10 = ADM Burn Time
			# 11 = ADM Max Cycles
			# 12 = ADM Wait Time 1
			# 13 = ADM Wait Time 2
			# 14 = ADM Enable/Disable
			# 15 = COMMS Cycle Time
			# 16 = PLD Cycle Time
			# 17 = PLD Operation Mode
			# 18 = Camera Resolution (not implemented on board satellite)
			# 19 = Camera Exposition Time (not implemented on board satellite)
			# 20 = Camera Picture Save Time
			# 21 = PLD Enable/Disable
			# 22 = Change RTC
			# 23 = Save RAM parameters to FLASH
			ram_cmd = {\
			0: [0xa9,0xa9,self.command_data], \
			1: [0xa9,0xaa,self.command_data], \
			2: [0xa9,0xab,self.command_data], \
			3: [0xa9,0xac,self.command_data], \
			4: [0xa9,0xad,self.command_data], \
			5: [0xa9,0xae,self.command_data], \
			6: [0xa9,0xaf,self.command_data], \
			7: [0xa9,0xb1,self.command_data], \
			8: [0xa9,0xb2,self.command_data], \
			9: [0xa9,0xb3,self.command_data], \
			10: [0xa9,0xb4,self.command_data], \
			11: [0xa9,0xb5,self.command_data], \
			12: [0xa9,0xb6,self.command_data], \
			13: [0xa9,0xb7,self.command_data], \
			14: [0xa9,0xb8,self.command_data], \
			15: [0xa9,0xb9,self.command_data], \
			16: [0xa9,0xba,self.command_data], \
			17: [0xa9,0xbb,self.command_data], \
			18: [0xa9,0xbc,self.command_data], \
			19: [0xa9,0xbd,self.command_data], \
			20: [0xa9,0xbe,self.command_data], \
			21: [0xa9,0xbf,self.command_data], \
			22: [0xa9,0xc9,self.hour,self.minute,self.day,self.month,self.year], \
			23: [0xa9,0xca]}

			# commands to reset specific modules
			# 0 = obc reset
			# 1 = ax100 reset
			# 2 = eps reset
			# 3 = adm reset
			# 4 = adcs reset
			# 5 = pld reset
			rst_cmd = {\
			0: [0xb9,0xa9], \
			1: [0xb9,0xaa], \
			2: [0xb9,0xab], \
			3: [0xb9,0xac], \
			4: [0xb9,0xad], \
			5: [0xb9,0xae]}

			# commands to operate heater
			# 0 = htr auto
			# 1 = htr off
			# 2 = htr on
			htr_cmd = {\
			0: [0xc9,0x41], \
			1: [0xc9,0x44], \
			2: [0xc9,0x55]}

			# commands to operate AX100
			# 0 = tx disable
			# 1 = tx enable
			# 2 = send ram/flash parameters
			ax100_cmd = {\
			0: [0xe9,0xa9], \
			1: [0xe9,0xaa], \
			2: [0xe9,0xab]}

			# commands to operate Payload
			# 0 = download image
			# 1 = take image
			# 2 = pld full operation
			pld_cmd = {\
			0: [0xf9,0xa9], \
			1: [0xf9,0xaa], \
			2: [0xf9,0xab]}

			# GENERATE COMMAND DEPENDING ON USER SELECTIONS
			
			if self.cmd_type == 0:
				command = np.array(ram_cmd[self.choose_ram_cmd],np.uint8)
			elif self.cmd_type == 1:
				command = np.array(rst_cmd[self.choose_reset_cmd],np.uint8)
				#rstCmdFlag = True
			elif self.cmd_type == 2:
				command = np.array(htr_cmd[self.choose_htr_cmd],np.uint8)
			elif self.cmd_type == 3:
				command = np.array(ax100_cmd[self.choose_ax100_cmd],np.uint8)
			elif self.cmd_type == 4:
				command = np.array(pld_cmd[self.choose_pld_cmd],np.uint8)
				#pldCmdFlag = True

			# FORM CSP PACKET

			transmit = np.concatenate([byte4,byte3,byte2,byte1,verByte1,verByte2,command])		# concatenates CSP Header and command into single frame  

			# FORM FAKE CSP PACKET
			
			commandFake = np.array([0x00, 0x00],np.uint8)
			transmitFake = np.concatenate([byte4,byte3,byte2,byte1,verByte1,verByte2,commandFake])

			# CALCULATE COMMAND LENGTH

			self.packet_len = len(transmit)

			# TRANSMIT

			if self.test_cmd:
				# HARD CODE HERE WHATEVER TEST COMMAND YOU WANT
				test_commands = [
					[0xa9, 0xae, 68],
					[0xa9, 0xae, 69],
					[0xa9, 0xb8, 0],
					[0xa9, 0xb8, 0],
					[0xa9, 0xac, 41],
					[0xa9, 0xac, 42],
					[0xa9, 0xac, 43]
				]
				preample_test_cmd = np.concatenate([byte4,byte3,byte2,byte1,verByte1,verByte2])
				for command in test_commands:
					to_append = np.array(command,np.uint8)
					transmit = np.append(preample_test_cmd, to_append)
					self.message_port_pub(pmt.intern('msg_out'), pmt.cons(pmt.make_dict(), pmt.pmt_to_python.numpy_to_uvector(transmit)))
			elif pldCmdFlag or rstCmdFlag:
				# THIS IS OBSOLETE
				# IF ITS A PAYLOD OR RESET COMMAND, SEND 3 INVALID + 1 VALID + 2 INVALID
				for i in range(0,3):
					self.message_port_pub(pmt.intern('msg_out'), pmt.cons(pmt.make_dict(), pmt.pmt_to_python.numpy_to_uvector(transmitFake)))
				self.message_port_pub(pmt.intern('msg_out'), pmt.cons(pmt.make_dict(), pmt.pmt_to_python.numpy_to_uvector(transmit)))
				self.message_port_pub(pmt.intern('msg_out'), pmt.cons(pmt.make_dict(), pmt.pmt_to_python.numpy_to_uvector(transmitFake)))
				self.message_port_pub(pmt.intern('msg_out'), pmt.cons(pmt.make_dict(), pmt.pmt_to_python.numpy_to_uvector(transmitFake)))
			else:
				# IF ANY OTHER COMMAND, SEND N VALID COMMANDS (BURST)
				for i in range(0,7):
					self.message_port_pub(pmt.intern('msg_out'), pmt.cons(pmt.make_dict(), pmt.pmt_to_python.numpy_to_uvector(transmit)))
				#self.message_port_pub(pmt.intern('msg_out'), pmt.cons(pmt.make_dict(), pmt.pmt_to_python.numpy_to_uvector(transmitFake)))			

			# PRINT TO TERMINAL

			print "\n"
			print "Command: ", transmit
			print "\n"

			# BRING BACK RELAYS INTO RX MODE
			if self.test_cmd:
				time.sleep(0.93)										# if test command, wait a little bit longer
			else:
				time.sleep(0.93)									# if normal set of commands, wait a little less
			self.ser.write(bytearray([0xa0,0x01,0x00,0xa1]))		# sets relays in RX mode

	def work(self, input_items, output_items):
		pass
